<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="51dp"
        android:layout_height="47dp"
        app:srcCompat="@drawable/icons8_arrow_left_32"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="28dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="53dp"
        android:layout_height="46dp"
        app:srcCompat="@drawable/icons8_writing_32"
        tools:layout_editor_absoluteX="342dp"
        tools:layout_editor_absoluteY="24dp" />


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/screenshot"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="149dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="156dp"
        android:layout_height="33dp"
        android:gravity="center"
        android:text="Tran Hieu"
        android:textColor="#FF0000"
        android:textSize="25dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="126dp"
        tools:layout_editor_absoluteY="302dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="207dp"
        android:layout_height="56dp"
        android:gravity="center"
        android:text="Binh Long, Binh Phuoc"
        android:textSize="19dp"
        tools:layout_editor_absoluteX="102dp"
        tools:layout_editor_absoluteY="335dp" />

</androidx.constraintlayout.widget.ConstraintLayout>